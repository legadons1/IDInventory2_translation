-- ATTENTION: Ne pas remplacer : %s (c'est pour les valeurs).

local LANG_EN = idinv.lang:Register("fr")

--[[---------
  WORDS
-----------]]
LANG_EN:addTranslation("inventory", "Inventaire")
LANG_EN:addTranslation("bank", "Banque")
LANG_EN:addTranslation("shipment", "Packtage")
LANG_EN:addTranslation("ammo", "Munitions")
LANG_EN:addTranslation("food", "Nourriture")
LANG_EN:addTranslation("take", "Prendre")
LANG_EN:addTranslation("use", "Utiliser")
LANG_EN:addTranslation("drop", "Lâcher")
LANG_EN:addTranslation("eat", "Manger")
LANG_EN:addTranslation("search", "Rechercher")
LANG_EN:addTranslation("admin", "Admin")
LANG_EN:addTranslation("users", "Utilisateurs")
LANG_EN:addTranslation("configure", "Configurer")
LANG_EN:addTranslation("trade", "Echanger")
LANG_EN:addTranslation("settings", "Options")

--[[---------
  PHRASES
-----------]]
LANG_EN:addTranslation("viewInventories", "Voir les inventaires")
LANG_EN:addTranslation("spawnedWeapon", "Prendre l'arme")
LANG_EN:addTranslation("moneyPrinter", "Imprimante d'argent")
LANG_EN:addTranslation("viewingUserInventories", "Voir l'inventaire de %s")
LANG_EN:addTranslation("shiftEToOpen", "%s + %s pour ouvrir")

--[[---------
  SENTENCES
-----------]]
LANG_EN:addTranslation("itemNotWhitelisted", "L'article n'est pas autorisé.")
LANG_EN:addTranslation("weaponEquipped", "Arme équipée.")
LANG_EN:addTranslation("itemUsed", "Article utilisé.")
LANG_EN:addTranslation("unableToUse", "Impossible d'utiliser l'élément.")
LANG_EN:addTranslation("ammoUsed", "Munitions utilisées.")
LANG_EN:addTranslation("itemEaten", "Article mangé.")
LANG_EN:addTranslation("unableToEat", "Impossible de manger l'article.")
LANG_EN:addTranslation("itemDropped", "Objet laché.")
LANG_EN:addTranslation("unableToDrop", "Impossible de déposer l'élément.")
LANG_EN:addTranslation("updatedContainer", "Conteneur utilisateur mis à jour.")
LANG_EN:addTranslation("clearedContainer", "Conteneur nettoyé/vidé.")
